<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Profile</title>
</head>
<body>
    <h1>Profile</h1>

    <%- include('partials/nav') %> <br>
    
    <hr>
    <table border="1">
        <tr>
            <th>User Name</th>
            <th>Profile Name</th>
            <th>Phone Number</th>
            <th>Email</th>
            <th>Password</th>
            <th>Action</th>
        </tr>

        <% menus.forEach(el => { %>
         <tr>
            <td><img src="<%= el.imageURL %>" alt="<%= el.itemName %>" width="200px"></td>
            <td><%= el.itemName %></td>
            <td><%= el.description %></td>
            <td><%= el.price %></td>
            <td>
                <button>
                    <a href="">Order</a>
                </button>
            </td>
         </tr>
        <% }) %>
    </table>
    
</body>
</html>